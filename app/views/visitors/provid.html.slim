= set_page_title "#{t('layout.title')} : #{t('layout.specialization.provid')}"

p#notice
  = notice
h3.text-center.content-header = I18n.t "layout.specialization.provid"

= render "specializations_nav"

br/

.tab-content
  - @specializations.each_with_index do |specialization, index|
    div class=("tab-pane #{"active" if index == 0}") id="#{multi_word_id_trasnform(specialization.name)}"
      = raw specialization.specdatum.provid

      br/
      br/
      i.pull-right = last_updated_at(specialization.specdatum)
