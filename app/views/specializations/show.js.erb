while(i < sanitize_will_paginate.length) {
  console.log("Pre  => " + sanitize_will_paginate[i].href);
  sanitize_will_paginate[i].href = sanitize_will_paginate[i].href.replace(/_=[0-9]*&/, "");
  console.log("Post => " + sanitize_will_paginate[i].href);
  i += 1;
}